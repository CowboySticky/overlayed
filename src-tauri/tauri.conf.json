{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "overlayed",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "http": {
        "all": true,
        "request": true,
        "scope": [
          "https://streamkit.discord.com/overlay/token"
        ]
      },
      "window": {
        "startDragging": true,
        "setAlwaysOnTop": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.hacksore.not.maleware.overlayed",
      "icon": [
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "transparent": true,
        "decorations": false,
        "title": "overlayed-rust-test",
        "width": 420,
        "height": 600
      }
    ]
  }
}
