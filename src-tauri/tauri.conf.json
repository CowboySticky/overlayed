{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "overlayed",
    "version": "0.0.0"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/32x32.png",
      "iconAsTemplate": false
    },
    "allowlist": {
      "all": false,
      "shell": {
        "open": ".*"
      },
      "fs": {
        "scope": [
          "$APPDATA/*"
        ],
        "readFile": true,
        "writeFile": true,
        "exists": true
      },
      "http": {
        "all": true,
        "request": true,
        "scope": [
          "https://streamkit.discord.com/overlay/token"
        ]
      },
      "path": {
        "all": true
      },
      "window": {
        "setSize": true,
        "startDragging": true,
        "setAlwaysOnTop": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.hacksore.overlayed",
      "icon": [
        "icons/icon.icns",
        "icons/icon.ico",
        "icons/icon.png"
      ]
    },
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "transparent": true,
        "decorations": false,
        "title": "overlayed",
        "width": 420,
        "height": 600,
        "minHeight": 106,
        "minWidth": 64
      }
    ]
  }
}
