{
  "extends": [
    "plugin:react/recommended",
    "plugin:astro/recommended",
    "plugin:react/jsx-runtime",
    "custom"
  ],
  "plugins": [
    "import",
    "react"
  ],
  "overrides": [
    {
      "files": [
        "*.astro"
      ],
      "parser": "astro-eslint-parser",
      "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "extraFileExtensions": [
          ".astro"
        ]
      },
      "rules": {}
    }
  ]
}
